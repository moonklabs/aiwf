{
    "name": "aiwf",
    "version": "0.3.2",
    "description": "AI Workflow Framework for Claude Code with multi-language support (Korean/English)",
    "main": "index.js",
    "type": "module",
    "bin": {
        "aiwf": "./index.js",
        "aiwf-lang": "./language-cli.js"
    },
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/.bin/jest",
        "lang:status": "node language-cli.js status",
        "lang:set": "node language-cli.js set",
        "lang:reset": "node language-cli.js reset",
        "validate:commands": "node scripts/validate-commands.js"
    },
    "keywords": [
        "aiwf",
        "ai",
        "workflow",
        "framework",
        "claude-code",
        "multilingual",
        "korean",
        "english",
        "project-management"
    ],
    "author": "moonklabs",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aiwf/aiwf.git"
    },
    "bugs": {
        "url": "https://github.com/aiwf/aiwf/issues"
    },
    "homepage": "https://github.com/aiwf/aiwf#readme",
    "engines": {
        "node": ">=14.0.0"
    },
    "dependencies": {
        "chalk": "^5.3.0",
        "commander": "^11.1.0",
        "fs-extra": "^11.3.0",
        "ora": "^8.0.1",
        "prompts": "^2.4.2",
        "tar": "^6.2.1",
        "tiktoken": "^1.0.21"
    },
    "devDependencies": {
        "jest": "^30.0.3"
    }
}
