{
  "name": "cursor",
  "version": "1.0.0",
  "description": "Cursor IDE integration template for AIWF",
  "compatible_aiwf_version": ">=0.1.0",
  "features": {
    "feature_ledger": true,
    "ai_persona": true,
    "context_compression": true,
    "offline_cache": true,
    "multi_file_edit": true
  },
  "setup_commands": [],
  "files_to_copy": [
    {
      "source": "template/.cursorrules",
      "destination": ".cursorrules"
    },
    {
      "source": "template/.cursor",
      "destination": ".cursor"
    }
  ],
  "settings": {
    "aiwf_context_provider": true,
    "feature_aware_completions": true,
    "sprint_task_tracking": true
  }
}